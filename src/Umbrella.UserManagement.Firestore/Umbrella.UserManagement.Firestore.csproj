  <PackageLicenseFile>LICENSE</PackageLicenseFile>
  <RepositoryType>git</RepositoryType>
  <RepositoryUrl>https://github.com/fgaravaglia/Umbrella.UserManagement.Firestore</RepositoryUrl>
  <icon>umbrella_framework_icon.png</icon>
